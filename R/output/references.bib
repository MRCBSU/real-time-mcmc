@article{pitm_costeffectiveness_2013,
  title = {Cost-Effectiveness of Childhood Influenza Vaccination in {{England}} and {{Wales}}: {{Results}} from a Dynamic Transmission Model},
  shorttitle = {Cost-Effectiveness of Childhood Influenza Vaccination in {{England}} and {{Wales}}},
