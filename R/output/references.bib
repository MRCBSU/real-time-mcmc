  author = {Pebody, R. G. and Andrews, N. and Fleming, D. M. and McMENAMIN, J. and Cottrell, S. and Smyth, B. and Durnall, H. and Robertson, C. and Carman, W. and Ellis, J. and {Sebastian-Pillai}, P. and Zambon, M. and Kearns, C. and Moore, C. and Thomas, D. Rh. and Watson, J. M.},
