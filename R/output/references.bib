  shorttitle = {Uptake and Impact of a New Live Attenuated Influenza Vaccine Programme in {{England}}},
  author = {Pebody, R. G and Green, H. K. and Andrews, N. and Zhao, H. and Boddington, N. and Bawa, Z. and Durnall, H. and Singh, N. and Sunderland, A. and Letley, L. and Ellis, J. and Elliot, A. J. and Donati, M. and Smith, G. E. and {de Lusignan}, S. and Zambon, M.},
