@article{schonberger_guillainbarre_1979a,
  title = {{{GUILLAIN}}-{{BARRE SYNDROME FOLLOWING VACCINATION IN THE NATIONAL INFLUENZA IMMUNIZATION PROGRAM}}, {{UNITED STATES}}, 1976\textendash 1977},
  author = {Schonberger, Lawrence B. and Bregman, Dennis J. and {Sullivan-Bolyai}, John Z. and Keenlyside, Richard A. and Ziegler, Donald W. and Retailliau, Henry F. and Eddins, Donald L. and Bryan, John A.},
  year = {1979},
  month = aug,
  volume = {110},
