  keywords = {Attenuated,Child,Child; Preschool,England,Female,Human,Humans,Immunization Programs,Incidence,Influenza,Influenza Vaccines,Influenza; Human,Male,Pilot Projects,Preschool,Schools,Seasons,Vaccination,Vaccines,Vaccines; Attenuated},
