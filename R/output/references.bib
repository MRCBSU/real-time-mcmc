@article{pebody_uptake_2018,
  title = {Uptake and Impact of Vaccinating Primary School-Age Children against Influenza: Experiences of a Live Attenuated Influenza Vaccine Programme, {{England}}, 2015/16},
  shorttitle = {Uptake and Impact of Vaccinating Primary School-Age Children against Influenza},
  author = {Pebody, R. G and Sinnathamby, Mary A and Warburton, Fiona and Andrews, Nick and Boddington, Nicola L and Zhao, Hongxin and Yonova, Ivelina and Ellis, Joanna and Tessier, Elise and Donati, Matthew and Elliot, Alex J and Hughes, Helen E and Pathirannehelage, Sameera and Byford, Rachel and Smith, Gillian E and {de Lusignan}, Simon and Zambon, Maria},
