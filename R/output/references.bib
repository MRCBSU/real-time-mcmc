  journal = {medRxiv},
  language = {en}
}

@article{SaenzDynamicsInfluenzaVirus2010,
  title = {Dynamics of {{Influenza Virus Infection}} and {{Pathology}}},
  author = {Saenz, Roberto A. and Quinlivan, Michelle and Elton, Debra and MacRae, Shona and Blunden, Anthony S. and Mumford, Jennifer A. and Daly, Janet M. and Digard, Paul and Cullinane, Ann and Grenfell, Bryan T. and McCauley, John W. and Wood, James L. N. and Gog, Julia R.},
