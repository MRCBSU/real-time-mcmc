@article{tempia_quantifying_2018,
  title = {Quantifying How {{Different Clinical Presentations}}, {{Levels}} of {{Severity}} and {{Healthcare Attendance Shape}} the {{Burden}} of {{Influenza}}-{{Associated Illness}}: A {{Modeling Study}} from {{South Africa}}},
  shorttitle = {Quantifying How {{Different Clinical Presentations}}, {{Levels}} of {{Severity}} and {{Healthcare Attendance Shape}} the {{Burden}} of {{Influenza}}-{{Associated Illness}}},
  author = {Tempia, Stefano and Walaza, Sibongile and Moyes, Jocelyn and Cohen, Adam L. and McMorrow, Meredith L. and Treurnicht, Florette K. and Hellferscee, Orienka and Wolter, Nicole and von Gottberg, Anne and Nguweneza, Arthemon and McAnerney, Johanna M. and Dawood, Halima and Variava, Ebrahim and Madhi, Shabir A. and Cohen, Cheryl},
  year = {2018},
  month = dec,
  issn = {1537-6591},
  doi = {10/gfqtc2},
  abstract = {Background: Burden estimates of medically- and non-medically-attended influenza-associated illness across syndromes and levels of severity are lacking. Methods: We estimated the national burden of medically- and non-medically-attended influenza-associated illness among individuals with different clinical presentations (all-respiratory, all-circulatory and non-respiratory/non-circulatory) and levels of severity (mild, severe-non-fatal and fatal) using a combination of case-based (from laboratory-confirmed influenza surveillance) and ecological studies as well as data from healthcare utilization surveys in South Africa during 2013-2015. In addition, we compared estimates of medically-attended influenza-associated respiratory illness obtained from case-based and ecological studies. Rates were reported per 100,000 population. Results: The estimated mean annual number of influenza-associated illness episodes was 10,737,847 (19.8\% of 54,096,705 inhabitants). Of these episodes, 10,598,138 (98.7\%), 128,173 (1.2\%) and 11,536 (0.1\%) were mild, severe-non-fatal and fatal, respectively. 2,718,140 (25.6\%) mild, 56,226 (43.9\%) severe-non-fatal and 4,945 (42.8\%) fatal episodes were medically-attended. Influenza-associated respiratory illness accounted for 99.2\% (10,576,146), 65.5\% (83,941) and 33.7\% (3,893) of any mild, severe-non-fatal and fatal illness, respectively. Ecological and case-based estimates of medically-attended influenza-associated respiratory mild (rates: ecological: 1,778.8 vs. case-based: 1,703.3; \% difference: 4.4\%), severe-non-fatal (rates: ecological: 88.6 vs. case-based: 75.3; \% difference: 15.0\%) and fatal (rates: ecological: 3.8 vs. case-based: 3.5; \% difference: 8.4\%) illness were similar. Conclusion: There was a substantial burden of influenza-associated symptomatic illness, including severe-non-fatal and fatal illness and a large proportion was non-medically-attended. Estimates including only influenza-associated respiratory illness substantially underestimated influenza-associated severe-non-fatal and fatal illness. Ecological and case-based estimates were found to be similar for the compared categories.},
  file = {/home/edwin/Documents/research/zotero/storage/BISYW99K/Tempia et al. - Quantifying how Different Clinical Presentations, .pdf},
  journal = {Clinical Infectious Diseases: An Official Publication of the Infectious Diseases Society of America},
  language = {eng},
  pmid = {30508065}
}

@article{terbraak_differential_2008,
