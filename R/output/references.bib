@article{ClarkComparisonLiveInactivated1983,
  title = {A {{Comparison}} of {{Live}} and {{Inactivated Influenza A}} ({{H1N1}}) {{Virus Vaccines}}: 2. {{Long}}-{{Term Immunity}}},
