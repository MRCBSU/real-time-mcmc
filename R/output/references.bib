@article{baguelin_agespecific_2011,
  title = {Age-{{Specific Incidence}} of {{A}}/{{H1N1}} 2009 {{Influenza Infection}} in {{England}} from {{Sequential Antibody Prevalence Data Using Likelihood}}-{{Based Estimation}}},
  author = {Baguelin, Marc and Hoschler, Katja and Stanford, Elaine and Waight, Pauline and Hardelid, Pia and Andrews, Nick and Miller, Elizabeth},
  year = {2011},
