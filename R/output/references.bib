  journal = {American Journal of Epidemiology},
  keywords = {Adolescent,Adult,Age Factors,Child,Child; Preschool,Contact Tracing,Cross-Sectional Studies,Disease Transmission; Infectious,Family Characteristics,Female,Humans,Infant,Infant; Newborn,Male,Middle Aged,Models; Statistical,Netherlands,Respiratory Tract Infections,Social Behavior,Surveys and Questionnaires},
  language = {eng},
