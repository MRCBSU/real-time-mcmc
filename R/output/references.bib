  volume = {8},
  pages = {e74388},
  issn = {1932-6203},
  doi = {10/ggvp87},
  abstract = {We consider a set of sample counts obtained by sampling arbitrary fractions of a finite volume containing an homogeneously dispersed population of identical objects. We report a Bayesian derivation of the posterior probability distribution of the population size using a binomial likelihood and non-conjugate, discrete uniform priors under sampling with or without replacement. Our derivation yields a computationally feasible formula that can prove useful in a variety of statistical problems involving absolute quantification under uncertainty. We implemented our algorithm in the R package dupiR and compared it with a previously proposed Bayesian method based on a Gamma prior. As a showcase, we demonstrate that our inference framework can be used to estimate bacterial survival curves from measurements characterized by extremely low or zero counts and rather high sampling fractions. All in all, we provide a versatile, general purpose algorithm to infer population sizes from count data, which can find application in a broad spectrum of biological and physical problems.},
  file = {/home/edwin/Documents/research/zotero/storage/MJYB7CWM/Comoglio et al. - 2013 - Bayesian Inference from Count Data Using Discrete .pdf;/home/edwin/Documents/research/zotero/storage/TM5HIVRH/Comoglio et al. - 2013 - Bayesian Inference from Count Data Using Discrete .pdf;/home/edwin/Documents/research/zotero/storage/RWYLEDNL/article.html},
  journal = {PLOS ONE},
  keywords = {Algorithms,Bacteria,Bayesian method,Binomials,Microbial ecology,negative binomial,Probability distribution,Pseudomonas fluorescens,Simulation and modeling},
  language = {en},
  number = {10}
}

