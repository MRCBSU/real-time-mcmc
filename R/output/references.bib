@article{lau_excess_2019,
  title = {Excess Influenza Hospital Admissions and Costs Due to the 2009 {{H1N1}} Pandemic in {{England}}},
  author = {Lau, Krystal and Hauck, Katharina and Miraldo, Marisa},
  year = {2019},
  month = feb,
