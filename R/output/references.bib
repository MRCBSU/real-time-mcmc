  journal = {Statistics and Computing},
  keywords = {Adaptive direction sampling,Adaptive Markov chain Monte Carlo,Differential evolution,Evolutionary Monte Carlo,Metropolis algorithm,Parallel computing,Theophylline kinetics},
  language = {en},
