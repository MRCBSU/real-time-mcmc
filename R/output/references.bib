@article{andreasen_epidemiologic_2008,
  title = {Epidemiologic {{Characterization}} of the 1918 {{Influenza Pandemic Summer Wave}} in {{Copenhagen}}: {{Implications}} for {{Pandemic Control Strategies}}},
  shorttitle = {Epidemiologic {{Characterization}} of the 1918 {{Influenza Pandemic Summer Wave}} in {{Copenhagen}}},
  author = {Andreasen, Viggo and Viboud, C{\'e}cile and Simonsen, Lone},
  year = {2008},
  month = jan,
  volume = {197},
  pages = {270--278},
  issn = {0022-1899},
  doi = {10/cs3b7m},
  abstract = {Background The 1918\textendash 1919 A/H1N1 influenza pandemic killed \textasciitilde 50 million people worldwide. Historical records suggest that an early pandemic wave struck Europe during the summer of 1918. Methods We obtained surveillance data that were compiled weekly, during 1910\textendash 1919, in Copenhagen, Denmark; the records included medically treated influenza-like illnesses (ILIs), hospitalizations, and deaths by age. We used a Serfling seasonal regression model to quantify excess morbidity and mortality, and we estimated the reproductive number (R) for the summer, fall, and winter pandemic waves. Results A large epidemic occurred in Copenhagen during the summer of 1918; the age distribution of deaths was characteristic of the 1918\textendash 1919 A/H1N1 pandemic overall. That summer wave accounted for 29\%\textendash 34\% of all excess ILIs and hospitalizations during 1918, whereas the case-fatality rate (0.3\%) was many-fold lower than that of the fall wave (2.3\%). Similar patterns were observed in 3 other Scandinavian cities. R was substantially higher in summer (2.0 \textendash 5.4) than in fall (1.2\textendash 1.6) in all cities. Conclusions The Copenhagen summer wave may have been caused by a precursor A/H1N1 pandemic virus that transmitted efficiently but lacked extreme virulence. The R measured in the summer wave is likely a better approximation of transmissibility in a fully susceptible population and is substantially higher than that found in previous US studies. The summer wave may have provided partial protection against the lethal fall wave.},
  file = {/home/edwin/Documents/research/zotero/storage/46ZE3589/Andreasen et al. - 2008 - Epidemiologic Characterization of the 1918 Influen.pdf},
  journal = {The Journal of infectious diseases},
  number = {2},
  pmcid = {PMC2674012},
  pmid = {18194088}
}

