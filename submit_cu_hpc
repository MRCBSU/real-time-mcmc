#!/bin/bash
#SBATCH -J trial_hpc_batch
#SBATCH -A MRC-BSU-SL2-CPU
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=14
#SBATCH --time=36:00:00
#! What types of email messages do you wish to receive?
#SBATCH --mail-type=FAIL
#SBATCH --mail-type=TIME_LIMIT
#SBATCH --mail-type=END
#! Uncomment this to prevent the job from being requeued (e.g. if
#! interrupted by node failure or system downtime):
#SBATCH --no-requeue
#SBATCH --verbose

#SBATCH --array=1-3
#SBATCH -p skylake


#SBATCH --output=model_runs_2020prevjobs_%a.out
#SBATCH --error=model_runs_2020prevjobs_%a.err

workdir="$SLURM_SUBMIT_DIR/model_runs/20201008/Prev"

if [ $SLURM_ARRAY_TASK_ID -eq 2 ]
then
    workdir="${workdir}EstR1relax"
elif [ $SLURM_ARRAY_TASK_ID -eq 3 ]
then 
    workdir="${workdir}EstR1tight"
fi
workdir="${workdir}_ifr_60cutoff6day_matrices_20201002_deaths"

module load gcc/9 intel/compilers/2020.2

echo $workdir

cd $workdir

../../../rtm_hpc
